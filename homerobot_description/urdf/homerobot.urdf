	      <child link="wheel_2"/>
	      <origin rpy="2.09 1.57 -1.57" xyz="0.346 -0.2 0"/>
	      <joint_properties damping="0.0" friction="0.0"/>
       </joint>
       <joint name="joint_base_wheel3" type="continuous">
